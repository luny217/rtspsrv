<?xml version="1.0" encoding="utf-8"?>
<config>
	<server_ip>192.168.30.50</server_ip>
	<server_port>80</server_port>
	<need_auth>0</need_auth>
	<auth_user>root</auth_user>
	<auth_pass>pass</auth_pass>
	<discoverable>1</discoverable>
	<merge_source>1</merge_source>
	<log_enable>0</log_enable>
	<information>
		<Manufacturer>HB IPC TEST</Manufacturer>
		<Model>IP Camera Test</Model>
		<FirmwareVersion>2.4</FirmwareVersion>
		<SerialNumber>123456</SerialNumber>
		<HardwareId>1.0</HardwareId>
	</information>
	<video_source_config>
		<x min="0" max="100" />
		<y min="0" max="100" />
		<width min="320" max="1280" />
		<height min="240" max="720" />
	</video_source_config>	
	<profile>
		<ProfileToken>1</ProfileToken>
		<video_source>
			<width>1280</width>
			<height>720</height>
		</video_source>
		<video_encoder>
			<width>1280</width>
			<height>720</height>
			<quality>4</quality>
			<session_timeout>10</session_timeout>			
			<framerate>25</framerate>
			<encoding_interval>50</encoding_interval>
			<bitrate_limit>4096</bitrate_limit>
			<encoding>H264</encoding>
			<h264>
				<gov_length>50</gov_length>
				<h264_profile>Main</h264_profile>
			</h264>
		</video_encoder>
		<audio_source></audio_source>
		<audio_encoder>
			<session_timeout>10</session_timeout>
			<sample_rate>8</sample_rate>
			<bitrate>64</bitrate>
			<encoding>G711</encoding>
		</audio_encoder>
		<stream_uri>rtsp://192.168.30.50/MainStream</stream_uri>
	</profile>	
	<profile>
		<ProfileToken>2</ProfileToken>
		<video_source>
			<width>1280</width>
			<height>720</height>
		</video_source>
		<video_encoder>
			<width>704</width>
			<height>576</height>
			<quality>4</quality>
			<session_timeout>10</session_timeout>			
			<framerate>25</framerate>
			<encoding_interval>50</encoding_interval>
			<bitrate_limit>1024</bitrate_limit>
			<encoding>H264</encoding>
			<h264>
				<gov_length>50</gov_length>
				<h264_profile>Main</h264_profile>
			</h264>
		</video_encoder>
		<audio_source></audio_source>
		<audio_encoder>
			<session_timeout>10</session_timeout>
			<sample_rate>8</sample_rate>
			<bitrate>64</bitrate>
			<encoding>G711</encoding>
		</audio_encoder>
		<stream_uri>rtsp://192.168.30.50/SubStream</stream_uri>
	</profile>
	<scope>onvif://www.onvif.org/Profile/Streaming</scope>
	<scope>onvif://www.onvif.org/location/country/china</scope>
	<scope>onvif://www.onvif.org/type/video_encoder</scope>
	<scope>onvif://www.onvif.org/name/IP-Camera</scope>
	<scope>onvif://www.onvif.org/hardware/HI3518C</scope>	
	<event>
		<renew_interval>60</renew_interval>	
		<simulate_enable>1</simulate_enable>
		<simulate_interval>10</simulate_interval>
	</event>
	<ptz>
		<enable>1</enable>
		<node>
			<FixedHomePosition>0</FixedHomePosition>
			<SupportedPTZSpaces>
				<AbsolutePanTiltPositionSpace>
					<XRange min="-1.0" max="1.0" />
					<YRange min="-1.0" max="1.0" />
				</AbsolutePanTiltPositionSpace>	
				<AbsoluteZoomPositionSpace>
					<XRange min="0.0" max="1.0" />
				</AbsoluteZoomPositionSpace>
				<RelativePanTiltTranslationSpace>
					<XRange min="-1.0" max="1.0" />
					<YRange min="-1.0" max="1.0" />
				</RelativePanTiltTranslationSpace>	
				<RelativeZoomTranslationSpace>
					<XRange min="-1.0" max="1.0" />
				</RelativeZoomTranslationSpace>
				<ContinuousPanTiltVelocitySpace>
					<XRange min="-1.0" max="1.0" />
					<YRange min="-1.0" max="1.0" />
				</ContinuousPanTiltVelocitySpace>
				<ContinuousZoomVelocitySpace>
					<XRange min="-1.0" max="1.0" />
				</ContinuousZoomVelocitySpace>
				<PanTiltSpeedSpace>
					<XRange min="0.0" max="1.0" />
				</PanTiltSpeedSpace>
				<ZoomSpeedSpace>
					<XRange min="0.0" max="1.0" />
				</ZoomSpeedSpace>						
			</SupportedPTZSpaces>
			<PTZConfiguration>
				<DefaultPTZSpeed PanTiltX="0.5" PanTiltY="0.5" Zoom="0.5" />
				<DefaultPTZTimeout>5</DefaultPTZTimeout>
				<PanTiltLimits>
					<XRange min="-1.0" max="1.0" />
					<YRange min="-1.0" max="1.0" />		
				</PanTiltLimits>	
				<ZoomLimits>
					<XRange min="0.0" max="1.0" />
				</ZoomLimits>
				<TimeoutRange>
					<XRange min="1" max="60" />
				</TimeoutRange>		
			</PTZConfiguration>
		</node>				
	</ptz>	
</config>